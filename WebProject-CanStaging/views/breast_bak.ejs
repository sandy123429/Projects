<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>CanStaging-breast</title>
        <!-- jquery -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
        
		<!-- Latest compiled and minified CSS -->
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

		<!-- Optional theme -->
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">

		<!-- Latest compiled and minified JavaScript -->
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
	</head>

	<body>
		<% include ./partials/header %>
		

		<div class="container">
			<ul class="nav nav-tabs">
				<li role="presentation"><a href="/main?name=<%=user %>"><%=labelList[0]%></a></li>
				<li role="presentation" class="active"><a href="#"><%=labelList[1]%></a></li>
				<li role="presentation"><a href="#"><%=labelList[2]%></a></li>
				<li role="presentation"><a href="#"><%=labelList[3]%></a></li>
				<li role="presentation"><a href="#"><%=labelList[4]%></a></li>
				<li role="presentation"><a href="#"><%=labelList[5]%></a></li>
			</ul>
			
			  <br>
			  	<% for (var i=0;i<2;i++){ %>
			  		<form class="form-horizontal">
				  	<div class="form-group">
					    <label for=<%= ans[i]%> class="col-sm-6 control-label; text-left">
					    	<%= q[i]%>
					    </label>
					    <div class="col-xs-offset-2 col-sm-1">
				    	<label class="radio-inline">
				      			<input type="radio" name=<%= ans[i]%> value="1"><%= a[i*2]%>
				      	</label>
					    </div>
					    <div class="col-xs-offset-1 col-sm-2">
					    	<label class="radio-inline active">
					      		<input type="radio" name=<%= ans[i]%> value="0" checked><%= a[i*2+1]%>
					      	</label>
					    </div>
				    </div>
				    </form>
			    <% } %>

			    <form class="form-horizontal">
				  	<div class="form-group">
					    <label for=<%= ans[2]%> class="col-sm-6 control-label; text-left">
					    	<%= q[2]%>
					    </label>
					    <div class="col-xs-offset-2 col-sm-2">
			     			<input type="text" class="form-control" name=<%= ans[2]%>  placeholder="cm">
			    		</div>
				    </div>
			    </form>

			  	<% for (var i=3;i<9;i++){ %>
			  		<form class="form-horizontal">
				  	<div class="form-group">
					    <label for=<%= ans[i]%> class="col-sm-6 control-label; text-left">
					    	<%= q[i]%>
					    </label>
					    <div class="col-xs-offset-2 col-sm-1">
				    	<label class="radio-inline">
				      			<input type="radio" name=<%= ans[i]%> value="1"><%= a[i*2]%>
				      	</label>
					    </div>
					    <div class="col-xs-offset-1 col-sm-2">
					    	<label class="radio-inline active">
					      		<input type="radio" name=<%= ans[i]%> value="0" checked><%= a[i*2+1]%>
					      	</label>
					    </div>
				    </div>
				    </form>
			    <% } %>

			    <form class="form-horizontal">
				  	<div class="form-group">
					    <label for=<%= ans[9]%> class="col-sm-6 control-label; text-left">
					    	<%= q[9]%>
					    </label>
					    <div class="col-xs-offset-2 col-sm-2">
			     			<input type="text" class="form-control" name=<%= ans[9]%>  placeholder="1~4">
			    		</div>
				    </div>
			    </form>

			  	<% for (var i=10;i<q.length;i++){ %>
			  		<form class="form-horizontal">
				  	<div class="form-group">
					    <label for=<%= ans[i]%> class="col-sm-6 control-label; text-left">
					    	<%= q[i]%>
					    </label>
					    <div class="col-xs-offset-2 col-sm-1">
				    	<label class="radio-inline">
				      			<input type="radio" name=<%= ans[i]%> value="1"><%= a[i*2]%>
				      	</label>
					    </div>
					    <div class="col-xs-offset-1 col-sm-2">
					    	<label class="radio-inline active">
					      		<input type="radio" name=<%= ans[i]%> value="0" checked><%= a[i*2+1]%>
					      	</label>
					    </div>
				    </div>
				    </form>
			    <% } %>


			<form class="form-horizontal" method="POST" enctype="application/json" action="http://nodejs-tnmproj.rhcloud.com/store?name=<%=user %>">
				<% for (var i=0;i<result.length;i++){ %>
			  	<div class="form-group">
			    <label class="col-sm-6 control-label; text-left">
			    	<%= result[i]%>
			    </label>
			    <div class="col-xs-offset-2 col-sm-1">
			    	<label class="radio-inline active">
			      			<input readonly name=<%= finalresult[i]%>></input>
			      	</label>
			    </div>
			    </div>
			    <% } %>
			    <div class="form-group">
			    <div class="col-sm-offset-10 col-sm-2">
			      <button type="submit" class="btn btn-default"><%= send%></button>
			      <br><br><br><br><br><br>
			    </div>
			  </div>
			</form>
		</div>


		<script>
			function getRadioValue(radioq)
			{
			    var elements = document.getElementsByName(radioq);
			    for (var i = 0, l = elements.length; i < l; i++)
			    {
			        if (elements[i].checked)
			        {
			            return elements[i].value;
			        }
			    }
			}
			function Tsize(tsize){
				if (tsize==0)
					return 1;
				else if (tsize>=0.11 && tsize <=0.5)
					return 2;
				else if (tsize>=0.51 && tsize <=1)
					return 3;
				else if (tsize>=1.01 && tsize <=2)
					return 4;
				else if (tsize>=0 && tsize <=0.1)
					return 5;
				else if (tsize>=2.01 && tsize <=5)
					return 6;
				else if (tsize>=5.01 && tsize <=15.9)
					return 7;
			}

			function update(){
				if (getRadioValue('ty')==1)
					document.getElementsByName('TNM')[0].value='y';
				else
					document.getElementsByName('TNM')[0].value=' ';
				if (getRadioValue('tpc')==1)
					document.getElementsByName('TNM')[0].value=document.getElementsByName('TNM')[0].value.concat('p');
				else
					document.getElementsByName('TNM')[0].value=document.getElementsByName('TNM')[0].value.concat('c');
				// if (document.getElementsByName('ans[1]'))
				
				setTimeout('update()',1000);
			}
			update();


		</script>
	</body>
</html>

